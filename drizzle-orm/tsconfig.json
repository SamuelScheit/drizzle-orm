{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": [
				"src/*"
			]
		},
		"declaration": true,
		"outDir": "dist",
		"noEmit": true,
		"noImplicitOverride": false,
		"noImplicitAny": false,
		"strict": false,
	},
	"include": [
		"src",
		"scripts"
	]
}